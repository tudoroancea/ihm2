{
    "configurations": [
        {
            "name": "Mac",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/install/ihm2/include/ihm2/**",
                "${env:HOME}/miniforge3/envs/ihm2/include/**",
                "${env:ACADOS_SOURCE_DIR}/include/**"
            ],
            "browse": {
                "path": [
                    "${workspaceFolder}/**",
                    "${workspaceFolder}/install/ihm2/include/ihm2/**",
                    "${env:HOME}/miniforge3/envs/ihm2/include/**",
                    "${env:ACADOS_SOURCE_DIR}/include/**"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            },
            "defines": [
                "TRACK_DATABASE_PATH=\"${workspaceFolder}/src/track-database/track_database/data/\""
            ],
            "macFrameworkPath": [
                "/Library/Developer/CommandLineTools/SDKs/MacOSX.sdk/System/Library/Frameworks"
            ],
            "compilerPath": "${env:HOME}/miniforge3/envs/ihm2/bin/clang",
            "intelliSenseMode": "macos-clang-arm64",
            "compileCommands": "${workspaceFolder}/build/compile_commands.json"
        }
    ],
    "version": 4
}